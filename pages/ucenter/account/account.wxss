/* pages/ucenter/account/account.wxss */
page{
  background-color: #eeeeee;
}
.account_t{
  background: #fff;
  padding-bottom: 40rpx;
}

.withdraw{
  font-size: 46rpx;
  text-align: center;
  padding-top: 50rpx;
  position: relative;
}
.withdraw image{
  position: absolute;
  height: 50rpx;
  width: 50rpx;
  right: 20rpx;
  top: 60rpx;
}
.other_money{
  padding: 20rpx 0;
  overflow: hidden
}
.other_money text{
  padding-left: 100rpx;
  width: 250rpx;
  line-height: 55rpx;
  float: left;
  color: #666;
}
.other_money text:last-child{
  float: right;
}
button{
  color: #fff;
  margin: 15rpx 60rpx 0;
}

.account_list{
  background: #fff;
  margin-top: 20rpx;
  display: flex;
  height: 140rpx;
  align-items: center
}
.title_explain{
  width: 520rpx;
  padding-left: 40rpx;
}
.explain{
  font-size: 30rpx;
}

.state{
  width: 120rpx;
}
.go_to{
  width: 60rpx;
  height: 40rpx;
  color: #999;
}
.go_to image{
  width: 40rpx;
  height: 40rpx;
}
